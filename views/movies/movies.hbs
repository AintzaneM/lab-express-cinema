<h1>Ironhack Cinema</h1>
<p>Click on the movies to check the showtimes!</p>


    <div class="container-listmovies">
        {{#each moviesArr}}

        <div class="info-movie">
            <img src= "{{this.image}}">
            <p><strong>{{this.title}} </strong></p>

            <a class="seeDetails" href="/movies/{{_id}}">See more</a>
        </div>
        {{/each}}
    </div>

